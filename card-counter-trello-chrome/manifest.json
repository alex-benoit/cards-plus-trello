{
  "manifest_version": 2,
  "name": "Trello Card Counter",
  "version": "2.0.0",
  "description": "Card counter and card ID's for easy collaboration and project management",
  "icons": { "128": "trelloext.png" },
  "content_scripts": [{
    "run_at" :"document_end",
    "matches": ["https://trello.com/*"],
    "js": ["trello-override.js"],
    "css": ["trello-override.css"]
  }]
}
